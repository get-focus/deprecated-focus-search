{
    "name": "focus-search",
    "version": "3.0.0-beta28",
    "description": "Serach FOCUS",
    "main": "src/index.js",
    "scripts": {
        "test": "BABEL_ENV=test mocha src/**/__tests__/**/*.js",
        "test:watch": "BABEL_ENV=test mocha src/**/__tests__/**/*.js --watch",
        "test-windows:watch": "better-npm-run test",
        "start": "better-npm-run dev-server",
        "build:node": "better-npm-run babelify && better-npm-run copy-sass ",
        "build": "npm run build:node",
        "build:watch": "better-npm-run watch",
        "prepublish": "npm run build"
    },
    "betterScripts": {
        "test": {
            "command": "mocha src/**/__tests__/**/*.js --watch",
            "env": {
                "BABEL_ENV": "test",
                "DEV": true,
                "SOURCE_MAP": false,
                "ENTRY_FILE_PATH": "./src/example/index.js",
                "PAGE_TITLE": "Focus entity dev page",
                "MINIMIFY": false,
                "API_PORT": 9999,
                "PACKAGE_JSON_PATH": "../",
                "ANCHOR_CLASS": "focus-search-example"
            }
        },
        "dev-server": {
            "command": "node ./server.js",
            "env": {
                "DEV": true,
                "SOURCE_MAP": false,
                "ENTRY_FILE_PATH": "./src/example/index",
                "PAGE_TITLE": "Focus entity dev page",
                "MINIMIFY": false,
                "API_PORT": 9999,
                "PACKAGE_JSON_PATH": "../",
                "ANCHOR_CLASS": "focus-search-example"
            }
        },
        "watch": {
            "command": "webpack --watch ./src/example/index.js",
            "env": {
                "DEV": true,
                "SOURCE_MAPS": false,
                "ENTRY_FILE_PATH": "./src/example/index",
                "PAGE_TITLE": "Focus entity dev page",
                "MINIMIFY": false,
                "API_PORT": 9999,
                "PACKAGE_JSON_PATH": "../",
                "ANCHOR_CLASS": "focus-search-example"
            }
        },
        "babelify": {
            "env": {
                "BABEL_ENV": "production"
            },
            "command": "node node_modules/babel-preset-focus/babelify.js"
        },
        "copy-sass": {
            "command": "cd src && find . -name *.scss | xargs tar cvf - | tar xfp - -C ../"
        }
    },
    "keywords": [
        "react",
        "redux",
        "data",
        "focus"
    ],
    "author": "FOCUS <focus@kleegroup.com>",
    "license": "MIT",
    "dependencies": {
        "focus-components": "3.0.0-rc50"
    },
    "peerDependencies": {
        "i18next": ">= 4.1.0",
        "lodash": ">= 4.15.0",
        "material-design-lite": "1.2.1",
        "material-design-icons-iconfont": ">= 2.0.4",
        "moment": ">= 2.14.1",
        "react": ">= 15.3.0 < 16.0.0",
        "react-dom": ">= 15.3.0 < 16.0.0",
        "react-redux": ">= 4.4.5",
        "redux": ">= 3.6.0",
        "redux-thunk": ">= 2.1.0"
    },
    "devDependencies": {
        "autoprefixer-loader": "^3.2.0",
        "babel-core": "^6.7.6",
        "babel-eslint": "^6.0.2",
        "babel-loader": "^6.2.4",
        "babel-plugin-flow-comments": "^6.3.19",
        "babel-polyfill": "^6.5.0",
        "babel-preset-focus": "^0.6.1",
        "better-npm-run": "0.0.6",
        "body-parser": "^1.14.2",
        "chai": "^3.5.0",
        "chai-subset": "^1.2.0",
        "css-loader": "^0.23.1",
        "enzyme": "^2.4.1",
        "express": "^4.13.4",
        "extract-text-webpack-plugin": "^1.0.1",
        "faker": "^3.0.1",
        "file-loader": "^0.8.5",
        "focus-application": "^3.0.0-rc8",
        "html-webpack-plugin": "^2.8.1",
        "i18next": "^4.1.0",
        "isomorphic-fetch": "^2.2.1",
        "jsdom": "^8.3.1",
        "json-loader": "^0.5.4",
        "lodash": "^4.15.0",
        "material-design-icons-iconfont": "^2.0.5",
        "material-design-lite": "1.2.1",
        "mocha": "^2.4.5",
        "node-sass": "^3.4.2",
        "react": "^15.3.2",
        "react-addons-test-utils": "^15.3.2",
        "react-dom": "^15.3.2",
        "react-hot-loader": "^3.0.0-beta.1",
        "react-redux": "^4.4.5",
        "react-router": "^2.8.1",
        "redux": "^3.6.0",
        "redux-devtools": "^3.1.1",
        "redux-devtools-dispatch": "^2.0.1",
        "redux-devtools-dock-monitor": "^1.1.0",
        "redux-devtools-inspector": "^0.3.1",
        "redux-devtools-multiple-monitors": "^1.0.0",
        "redux-logger": "^2.5.0",
        "redux-slider-monitor": "^1.0.2",
        "redux-thunk": "^2.1.0",
        "remarkable": "^1.6.2",
        "sass-loader": "^3.1.2",
        "sinon": "^1.17.3",
        "sinon-chai": "^2.8.0",
        "source-map-loader": "^0.1.5",
        "style-loader": "^0.13.0",
        "url-loader": "^0.5.7",
        "webpack": "^1.12.13",
        "webpack-dev-server": "^1.14.0",
        "webpack-focus": "^0.12.2"
    }
}
